<section class="home">
    <h2>Productos</h2>
    <div class="contenedor-productos">
        {{#each products}}
        <div class="producto">
            <h3>{{this.title}}</h3>
            <p>{{this.description}}</p>
            <p>{{this.price}}</p>
        </div>
        {{/each}}
    </div>
</section>
